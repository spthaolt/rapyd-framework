{
    "name": "zofe/rapyd-framework",
    "type": "project",
    "description": "microframework for PHP 5.3 CRUD pattern",
    "homepage": "http://www.rapyd.com",
    "keywords": ["Rapyd", "Slim", "Eloquent", "Twig", "CRUD"],
    "license": "MIT",
    "authors": [
        {
            "name": "Felice Ostuni",
            "email": "felice.ostuni@gmail.com",
            "homepage": "http://www.feliceostuni.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "slim/slim": "2.*",
        "slim/views": "0.1.*",
        "twig/twig": "1.*",
        "illuminate/database": "4.0.*",
        "symfony/form": "2.3.*",
        "symfony/validator": "2.3.*",
        "symfony/config": "2.3.*",
        "symfony/translation": "2.3.*",
        "symfony/twig-bridge": "2.3.*"
    },
    "minimum-stability": "dev",
    "autoload": {
        "psr-0": { 
            "App": "src/",
            "Rapyd": "src/", 
            "Modules": "src/" 
        }
    }
}
